<h1>Today Reports</h1>
<div id="reports">
  <% @reports.each do |report| %>
      <div id='<%= dom_id(report)%>'>
        <%= report.user.name %>
        <%= report.report_date %>
        <%= link_to "View Detail",report_path(report) %>
        <%= link_to "Edit",edit_report_path(report) %>
      </div>
  <% end %>
</div>


