    <% if current_user %>
        <% if current_user.admin? %>
            <%= render partial: 'layouts/admin' %>
        <% elsif current_user.leader? %>
            <%= render partial: 'layouts/leader' %>
        <% else current_user.member? %>
            <%= render partial: 'layouts/member' %>
        <% end %>
    <% else %>
        <%= render partial: 'layouts/default' %>
    <% end %>